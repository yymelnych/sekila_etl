      SELECT first_name,
			last_name
      FROM [dbo].[dimActor]
      WHERE g_id= @g_id
			OR s_id = @s_id
      EXCEPT
      SELECT @first_name, @last_name
    )
  )
BEGIN
  UPDATE [dbo].[dimActor]
  SET first_name = @first_name,
	  last_name = @last_name,
	  updated = GETDATE()
  WHERE g_id= @g_id
		OR s_id = @s_id
END</property>
